#include <iostream>
using namespace std;

struct Point {
    float x;
    float y;
};

int main() {
    Point arr[7];
    int count = 0;

    for (int i = 0; i < 7; ++i) {
        cout << "Enter x and y for point " << i + 1 << ": ";
        cin >> arr[i].x >> arr[i].y;
    }

    for (int i = 0; i < 7; ++i) {
        if (arr[i].x > 0 && arr[i].y > 0) {
            count++;
        }
    }

    cout << "Number of points in the first quadrant: " << count << endl;

    return 0;
}
