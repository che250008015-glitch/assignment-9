#include <iostream>
using namespace std;
struct item {
    int id;
    float cost;
};
int main(){
    item arr[6] = {
        {1, 49.3},
        {2, 54.7},
        {3, 69.3},
        {4, 43},
        {5, 87},
        {6, 73.3},
    };
    cout << "Items with cost greater than 50.00:\n";
    for (int i = 0; i<6; i++){
        if (arr[i].cost > 50){
            cout<< "item id: "<<arr[i].id
            << ", Cost: " << arr[i].cost << endl;
        }
    }
    return 0;
}
